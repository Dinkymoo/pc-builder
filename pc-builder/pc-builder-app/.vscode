{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "files.eol": "\n",
  "eslint.enable": true,
  "eslint.ignoreUntitled": true,
  "eslint.onIgnoredFiles": "warn",
  "eslint.codeAction.disableRuleComment": {
    "enable": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}